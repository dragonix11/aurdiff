#Mantainer: M0Rf30
pkgname=gyp-svn
pkgver=1742
pkgrel=1
pkgdesc="GYP can Generate Your Projects."
url="http://code.google.com/p/gyp/"
arch=('i686' 'x86_64')   
makedepends=(svn)
depends=('python2' 'python2-setuptools')
license=(BSD2)
source=("svn+http://gyp.googlecode.com/svn/trunk/")

pkgver() {
  cd "$srcdir/trunk"
  svnversion | tr -d [A-z]
}

package() {
  cd "$srcdir/trunk"
  python2 setup.py install --root=$pkgdir
}
md5sums=('SKIP')
