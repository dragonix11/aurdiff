# Maintainer: Muflone <muflone@vbsimple.net>

pkgname=cpucompare
pkgver=0.3
pkgrel=1
pkgdesc="A GTK+ application to make comparisons between CPU models."
url="https://github.com/muflone/cpucompare"
arch=('any')
license=('GPL2')
depends=('python2' 'gtk3')
provides=('cpucompare')
source=("${pkgname}-${pkgver}.tar.gz"::"https://github.com/muflone/${pkgname}/archive/${pkgver}.tar.gz")
md5sums=('c7b1893174fedcc104a22d0548f4f893')
sha1sums=('12ddcaa2ea9a529f305b95306de666ac1e9a5d37')
sha256sums=('8252373748b1c676d462a55e1b3f67459dee5439179f9f39238fa6cb0b083379')

build() {
  cd "${srcdir}/${pkgname}-${pkgver}"
  python2 setup.py build
}

package() {
  cd "${srcdir}/${pkgname}-${pkgver}"
  python2 setup.py install --root "${pkgdir}"
}

