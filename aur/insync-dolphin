# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-dolphin
pkgver=1.0.22
pkgrel=1
pkgdesc="Dolphin integration for insync"
url="https://support.insynchq.com/forums/discussion/2208/new-insync-version-1-0-22-with-do-not-convert-feature-headless-app"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "kdebindings-python2" "kdebase-workspace")
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}_all.deb")
sha256sums=("baf610fbca45514380e078bb9163ea21f9f33b4b60c24e84dd9e789cd28c2195")
noextract=("${pkgname}-${pkgver}.deb")
install=$pkgname.install

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
