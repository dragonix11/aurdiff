package () {
  cd -- "$srcdir/$pkgname-$pkgver"
  install -Dm644 "en_XX@POSIX" "$pkgdir/usr/share/i18n/locales/en_XX@POSIX"
#   install -Dm755 "en_XX.sh" "$pkgdir/etc/profile.d/en_XX.sh"
}

